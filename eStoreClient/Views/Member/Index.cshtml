@model List<BusinessObject.Models.Member>

<h1>Members</h1>
<a asp-controller="Member" asp-action="Create" class="btn btn-primary">Create New Member</a>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>Company Name</th>
            <th>City</th>
            <th>Country</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.MemberId</td>
                <td>@member.Email</td>
                <td>@member.CompanyName</td>
                <td>@member.City</td>
                <td>@member.Country</td>
                <td>
                    <a asp-controller="Member" asp-action="Edit" asp-route-id="@member.MemberId">Edit</a> |
                    <a asp-controller="Member" asp-action="Delete" asp-route-id="@member.MemberId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>